{
  "name": "gazin",
  "version": "0.0.1",
  "description": "Teste na gazin",
  "scripts": {
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev": "cross-env DEBUG=app:* nodemon src/server.ts",
    "dev:debug": "cross-env DEBUG=app:* nodemon --inspect src/server.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --runInBand --watchAll --verbose",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "prepare-db-dev": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "prepare-db-test": "cross-env NODE_ENV=test node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "prepare-environment-docker": "yarn prepare-db-dev && yarn prepare-db-test && yarn dev"
  },
  "keywords": [],
  "author": "Max Rog√©rio",
  "license": "ISC",
  "dependencies": {
    "celebrate": "^12.2.0",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "typeorm-express-query-builder": "^1.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "superagent": "^5.3.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}